<mvc:View
	controllerName="cl.everis.cgr.actvinst.allCGRActvInstAll.controller.Master"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m"
	xmlns:u="sap.ui.unified"
	xmlns:semantic="sap.m.semantic">

	<semantic:MasterPage
		id="page"
		title="{masterView>/title}"
		navButtonPress="onNavBack"
		showNavButton="true">
		<semantic:subHeader>
			<Bar id="headerBar">
				<contentMiddle>
					<SearchField
						id="searchField"
						showRefreshButton="{= !${device>/support/touch} }"
						tooltip="{i18n>masterSearchTooltip}"
						width="100%"
						search="onSearch">
					</SearchField>
				</contentMiddle>
			</Bar>
		</semantic:subHeader>

		<semantic:content>
		<Tree
		    id="Tree" 
		    items="{path: 'treeModel>/'}">
			<!--<StandardTreeItem type="Active" press="onPressNode" title="{treeModel>text}"/>-->
			<CustomTreeItem type="Active" press="onPressNode">
				<FlexBox alignItems="Start" width="100%">
					<Label id="NodoLab1" text="{treeModel>id}" visible="false"></Label>
					<Label id="TextoLab1" text="{treeModel>text}"></Label>
				</FlexBox>
			</CustomTreeItem>
		</Tree>	
		</semantic:content>

	</semantic:MasterPage>

</mvc:View>